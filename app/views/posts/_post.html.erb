<div class="col-md-4 post">
	<p>
		<%=	image_tag post.image.url(:medium), class: 'uploaded-pic'%>
		<br>
		<div class='post_title'>
			<%= post.title %>
		</div>
		<span>@<%= link_to 'Location', post.location, post_location_path %></span>
		<% (post.tags).each do|tag| %>
			<span class='post_tags'>
				<%= link_to tag.text, tag %>
			</span>
		<% end %>
	</p>
</div>